    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "author": "Mike Antoniadis <ssbeefeater@gmail.com> (http://ssbeefeater.github.io/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ssbeefeater/react-typed.git"
  },
  "tags": [
